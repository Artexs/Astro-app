{
  "name": "10x-astro-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:e2e": "astro dev --mode test",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint2": "npm run lint:fix && npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run --coverage",
    "test:e2e": "playwright test",
    "db:migrate": "npx supabase migration up",
    "db:migrate:e2e": "cd supabase_e2e && npx supabase migration up",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:start:e2e": "npx supabase --workdir supabase_e2e start",
    "supabase:stop:e2e": "npx supabase --workdir supabase_e2e stop",
    "supabase:start:all": "npm run supabase:start && npm run supabase:start:e2e",
    "supabase:stop:all": "npm run supabase:stop && npm run supabase:stop:e2e",
    "docker:run": "npm run docker:stop && ./run-docker-container.sh",
    "docker:run:test": "npm run docker:stop && ./run-docker-container.sh .env.test",
    "docker:stop": "docker rm -f astro-app-container"
  },
  "dependencies": {
    "@astrojs/node": "^9.4.3",
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "^3.5.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.487.0",
    "openai": "^6.6.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@faker-js/faker": "^10.1.0",
    "@mswjs/http-middleware": "^0.10.3",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitest/coverage-v8": "^4.0.1",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "15.5.0",
    "msw": "^2.11.6",
    "prettier-plugin-astro": "0.14.1",
    "supabase": "^2.51.0",
    "supertest": "^7.1.4",
    "typescript-eslint": "8.28.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.1"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
